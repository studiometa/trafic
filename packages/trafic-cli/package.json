{
  "name": "@studiometa/trafic-cli",
  "version": "0.1.5",
  "description": "CLI to deploy projects to DDEV servers â€” preview environments from CI",
  "keywords": [
    "ci",
    "cli",
    "ddev",
    "deploy",
    "preview"
  ],
  "license": "MIT",
  "author": "Studio Meta",
  "repository": {
    "type": "git",
    "url": "https://github.com/studiometa/trafic",
    "directory": "packages/trafic-cli"
  },
  "bin": {
    "trafic": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest run --exclude 'test/integration/**'",
    "test:watch": "vitest --exclude 'test/integration/**'",
    "test:ci": "vitest run --coverage --exclude 'test/integration/**'",
    "test:integration": "node test/integration/run.js",
    "test:integration:ci": "node test/integration/run.js --coverage",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
